double arithmetic(double a, double b, char* operator) {
  // Your Code here!
int ergebnis;

if(operator =="add")
{
 return a+b;
}

if(operator =="subtract")
{
 return a-b;
}

if(operator =="multiply")
{
 return a*b;
}

if(operator =="divide")
{
 return a/b;
}

}
